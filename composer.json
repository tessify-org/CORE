{
    "name": "tessify/core",
    "description": "The CORE of the Tessify project.",
    "authors": [
        {
            "name": "Nick Verheijen",
            "email": "verheijen.webdevelopment@gmail.com"
        }
    ],
    "version": "dev-master",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tessify\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tessify\\Core\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/support": "~6.2",
        "laravolt/avatar": "^3.0",
        "webpatser/laravel-uuid": "^3.0",
        "laracasts/flash": "^3.0",
        "cviebrock/eloquent-sluggable": "^6.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x"
    },
    "require-dev": {},
    "extra": {
        "laravel": {
            "providers": [
                "Tessify\\Core\\Providers\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "Tessify\\Core\\Facades\\CoreFacade",
                "Users": "Tessify\\Core\\Facades\\UsersFacade",
                "Skills": "Tessify\\Core\\Facades\\SkillsFacade",
                "Projects": "Tessify\\Core\\Facades\\ProjectsFacade",
                "ProjectStatuses": "Tessify\\Core\\Facades\\ProjectStatusesFacade",
                "ProjectCategories": "Tessify\\Core\\Facades\\ProjectCategoriesFacade",
                "ProjectResources": "Tessify\\Core\\Facades\\ProjectResourcesFacade",
                "WorkMethods": "Tessify\\Core\\Facades\\WorkMethodsFacade",
                "Tasks": "Tessify\\Core\\Facades\\TasksFacade",
                "TaskStatuses": "Tessify\\Core\\Facades\\TaskStatusesFacade",
                "TaskCategories": "Tessify\\Core\\Facades\\TaskCategoriesFacade",
                "TaskSeniorities": "Tessify\\Core\\Facades\\TaskSenioritiesFacade",
                "TeamMembers": "Tessify\\Core\\Facades\\TeamMembersFacade",
                "TeamMemberApplications": "Tessify\\Core\\Facades\\TeamMemberApplicationsFacade",
                "TeamRoles": "Tessify\\Core\\Facades\\TeamRolesFacade",
                "Comments": "Tessify\\Core\\Facades\\CommentsFacade",
                "Dates": "Tessify\\Core\\Facades\\DatesFacade",
                "Uploader": "Tessify\\Core\\Facades\\UploaderFacade"
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
